<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">

    <% include parts/head %>
  </head>
  <body>
    <!--<div class="data">
      <h1>New</h1>
    </div>-->

    <% include parts/nav %>
    <div class="container">
      <a href="/index" class="btn btn-danger">Back</a>
      <h1>Upload Picture!</h1>
      <div class="data data-new">
        <form action="/index" method="post" enctype="multipart/form-data">
          <div class="form-group">
            <label name="title"><h3>title</h3></label>
            <input type="text" id="title" name="title" class="form-control">
          </div>
          <div class="form-group">
            <label name="picture"><h3>picture</h3></label>
            <input type="file" accept="image/*" id="picture" name="picture" class="form-control">
          </div>
          <button type="submit" class="btn btn-success">Submit</button>
        </form>
      </div>
    </div>
  </body>
</html>
